{
  "name": "my-app",
  "version": "0.1.0",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": "12.13.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jquery": "^3.4.1",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "node": "^13.1.0",
    "popper.js": "^1.16.0",
    "react-router": "^5.1.2",
    "react-sticky": "^6.0.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "^2.1.1",
    "react-router-dom": "^5.1.2",
    "react-google-login": "^5.0.7",
    "react-datepicker": "^2.9.6"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev && react-scripts start",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "start:prod": "node server.js",
    "seed": "node scripts/seedDB.js",
    "scrape": "node client/src/webScrape.js",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build && react-scripts build",
    "postinstall": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}